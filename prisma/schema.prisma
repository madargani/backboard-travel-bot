generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          String    @id @default(cuid())
  clerkId     String    @unique
  assistantId String    @unique
  email       String
  name        String?
  threadIds   String[]
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}